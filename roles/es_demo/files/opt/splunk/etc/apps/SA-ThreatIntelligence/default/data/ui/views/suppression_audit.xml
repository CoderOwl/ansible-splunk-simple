<dashboard>
	<label>Suppression Audit</label>
	
	<row>
		<chart id="element1">
			<title>Suppressed Events Over Time - Last 24 Hours</title>
			
			<searchName>Suppressions - Currently Suppressed Events Over Time</searchName>
			
			<option name="charting.axisTitleX.text">time</option>
			<option name="charting.axisTitleY.text">count</option>
			
			<drilldown>
				<link>search?q=`suppressed_notables` | search rule_name="$click.name2$"&amp;earliest=$earliest$&amp;latest=$latest$</link>
			</drilldown>
		</chart>

		<chart id="element2">
			<title>Suppression History Over Time - Last 30 Days</title>
			
			<searchName>Suppressions - Suppression History Over Time</searchName>
			
			<option name="charting.axisTitleX.text">time</option>
			<option name="charting.axisTitleY.text">count</option>
			
			<drilldown>
				<link>search?q=`suppressed_notables` | search rule_name="$click.name2$"&amp;earliest=$earliest$&amp;latest=$latest$</link>
			</drilldown>
		</chart>
	</row>
	
	<row>
		<table id="element3">
			<title>Suppression Management Activity</title>
			
			<searchName>Suppressions - Suppression Management Activity</searchName>
			
			<option name="wrap">true</option>
			<option name="displayRowNumbers">false</option>
			<option name="dataOverlayMode">none</option>
			<option name="drilldown">row</option>
			
			<drilldown>
				<link>search?q=`suppression_audit` | search suppression="$row.suppression$"&amp;earliest=$earliest$&amp;latest=$latest$</link>
			</drilldown>
		</table>
	</row>
	
	<row>
		<table id="element4">
			<title>Expired Suppressions</title>
			
			<searchName>Suppressions - Expired Suppressions</searchName>
			
			<option name="wrap">true</option>
			<option name="displayRowNumbers">false</option>
			<option name="dataOverlayMode">none</option>
			<option name="drilldown">row</option>
			
            <drilldown>
              	<link>search?q=`suppression_audit-expired` | search suppression="$row.suppression$"&amp;earliest=$earliest$&amp;latest=$latest$</link>
			</drilldown>
		</table>
	</row>
	
</dashboard>