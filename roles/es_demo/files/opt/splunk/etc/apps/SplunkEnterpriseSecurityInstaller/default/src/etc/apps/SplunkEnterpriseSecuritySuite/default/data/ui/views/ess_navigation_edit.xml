<view template="dashboard.html" isDashboard="false">
   
  <label>Edit Navigation</label>
  
  <module name="AccountBar" layoutPanel="appHeader"/>
  <module name="AppBar" layoutPanel="navigationHeader"/>
  <module name="Message" layoutPanel="messaging">
    <param name="filter">*</param>
    <param name="clearOnJobDispatch">False</param>
    <param name="maxSize">2</param>
  </module>
  <module name="BreadCrumb" layoutPanel="viewHeader">
    <param name="options">
      <list>
        <param name="view">ess_configuration</param>
        <param name="label">Configure</param>
      </list>
      <list>
        <param name="label">Navigation</param>
      </list>
    </param>
  </module>

  <module name="IFrameInclude" layoutPanel="panel_row1_col1">
    <param name="src">/custom/SA-Utils/nav_editor/SplunkEnterpriseSecuritySuite/render</param>
    <param name="height">600</param>
  </module>

</view>