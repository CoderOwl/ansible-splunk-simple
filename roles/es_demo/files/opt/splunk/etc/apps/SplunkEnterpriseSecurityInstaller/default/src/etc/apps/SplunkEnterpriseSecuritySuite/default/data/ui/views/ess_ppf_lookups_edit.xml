<view template="dashboard.html" isDashboard="false">
  
  <label>Edit Per-Panel Filtering Lookup</label>
  
  <module name="AccountBar" layoutPanel="appHeader"/>
  <module name="AppBar" layoutPanel="navigationHeader"/>
  <module name="Message" layoutPanel="messaging">
    <param name="filter">*</param>
    <param name="clearOnJobDispatch">False</param>
    <param name="maxSize">2</param>
  </module>
  <module name="BreadCrumb" layoutPanel="viewHeader">
    <param name="options">
      <list>
        <param name="view">ess_configuration</param>
        <param name="label">Configure</param>
      </list>
      <list>
        <param name="view">ess_ppf_lookups_list</param>
        <param name="label">Per-Panel Filtering Lookups</param>
      </list>
      <list>
        <param name="label">Edit Per-Panel Filtering Lookup</param>
      </list>
    </param>
  </module>
  <module name="SOLNLookupEditor" layoutPanel="panel_row1_col1">
    <param name="listView">ess_ppf_lookups_list</param>
    <param name="titleNew">New Per-Panel Filtering Lookup</param>
    <param name="titleEdit">Edit Per-Panel Filtering Lookup</param>
    
  </module>

</view>