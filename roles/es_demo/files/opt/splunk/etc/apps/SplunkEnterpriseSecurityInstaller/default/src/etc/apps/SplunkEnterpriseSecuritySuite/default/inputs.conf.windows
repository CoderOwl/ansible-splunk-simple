## Configuration checks.
[configuration_check://confcheck_default_search_indexes]
handler = confcheck_default_search_indexes
interval = 0 3 * * *
default_severity = INFO
required_ui_severity = WARN
suppress =

[configuration_check://confcheck_default_search_all_non_internal]
handler = confcheck_default_search_all_non_internal
interval = 5 3 * * *
default_severity = INFO
required_ui_severity = WARN
suppress =

[configuration_check://confcheck_lookup_table_size]
handler = confcheck_lookup_table_size
interval = 10 3 * * *
default_severity = INFO
required_ui_severity = WARN
suppress =

[configuration_check://confcheck_script_errors]
handler = confcheck_script_errors
interval = 0 * * * *
default_severity = INFO
required_ui_severity = WARN
