
## Update the meta-data
[app_imports_update://update_es]
app_regex = ([DST]A-.*)|(Splunk_[DST]A_.*)|(SplunkEnterpriseSecuritySuite)
app_exclude_regex = sideview_utils
apps_to_update = SA-AccessProtection,Splunk_SA_CIM,SA-AuditAndDataProtection,SA-EndpointProtection,SA-IdentityManagement,SA-NetworkProtection,SA-ThreatIntelligence,SA-Utils,SA-Eventgen,SA-CommonInformationModel
# Leave this disabled, this input will be enabled in setup
disabled = 1

## Update data model acceleration settings.
[dm_accel_settings]
interval = 86400

[dm_accel_settings://Application_State]
acceleration = true
disabled = 1
manual_rebuilds = true

